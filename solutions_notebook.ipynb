    "# 258. Add Digits\n",
    "def addDigits(num: int):\n",
    "    while num > 9:\n",
    "        new = [int(x) for x in str(num)]\n",
    "        sum_list = sum(new)\n",
    "        num = sum_list\n",
    "\n",
    "    return num\n",
    "\n",
    "\n",
    "a = addDigits(48)\n",
    "print(a)"
